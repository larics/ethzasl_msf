<launch>
	<arg name="imu_topic" default="mavros/imu/data" />
	<arg name="position_topic" default="mavros/local_position/pose"/>

    <arg name="namespace" default="erl_husky"/>
    	
    <node name="msf_pose_sensor" pkg="msf_updates" type="pose_sensor" clear_params="true" output="screen"
        ns="$(arg namespace)">
          <remap from="msf_core/imu_state_input" to="$(arg imu_topic)" />
          <remap from="msf_updates/pose_input" to="$(arg position_topic)" />
          <rosparam file="$(find msf_updates)/msf_husky_params_pose.yaml"/>
    </node>
</launch>
